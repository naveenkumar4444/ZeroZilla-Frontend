{
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build"
        }
    ]
}